{
    "name": "pedramkousari/expire-command",
    "version": "1.0.0",
    "description": "A Laravel package for managing expiration commands",
    "keywords": [
        "laravel",
        "expire",
        "command"
    ],
    "type": "library",
    "require": {
        "php": "^8.2",
        "laravel/framework": "^11.9",
        "laravel/prompts": "^0.3.2"
    },
    "autoload": {
        "psr-4": {
            "Pedramkousari\\ExpireCommand\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "license": "MIT",
    "authors": [
        {
            "name": "Pedram Kousari",
            "email": "pedramkousari@gmail.com"
        }
    ],
    "homepage": "https://github.com/pedramkousari/expire-command",
    "support": {
        "issues": "https://github.com/pedramkousari/expire-command/issues",
        "source": "https://github.com/pedramkousari/expire-command"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pedramkousari\\ExpireCommand\\Providers\\ExpireCommandServiceProvider::class"
            ]
        }
    }
}